{
  "name": "sabio-web-api",
  "version": "1.0.0",
  "description": "Express based Node Api",
  "engines": {
    "node": ">=10.14.1"
  },
  "main": "server.js",
  "scripts": {
    "start": "SET SHOW_ROUTES=true && node --inspect .compiled/server.js",
    "compile": "concurrently \"npm:babel-app\" \"npm:notify\" ",
    "kill-node-debugger": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command \"%CD%\\killSomeNode.ps1\" ",
    "kill-node": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command \"%CD%\\killNode.ps1\" ",
    "~~~~~~~~ pre commit ~~~~~~~~": "",
    "lint": "eslint --color -f util/eslintFormatter.js ./**/*.js ",
    "lint-app": "eslint --color -f util/eslintFormatter.js app/**/*.js ",
    "babel-app": "babel app --out-dir .compiled --source-maps --watch --delete-dir-on-start",
    "babel-app-q": "babel app --out-dir .compiled",
    "~~~~~~~~ utilityies below ~~~~~~~~": "",
    "notify": "node util/watch.js",
    "lint-fix": "eslint --fix --color -f util/eslintFormatter.js . ",
    "postinstall": "npm run link-cmd",
    "link-cmd": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command \"%CD%\\link.ps1\" ",
    "spinup-die": "node shortlife.js",
    "check-integrity": "del-cli .compiled && babel app --out-dir .compiled --delete-dir-on-start && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "http://code.sabio.la:8080/tfs/SabioCollection/"
  },
  "author": "Sabio - Gregorio Rojas",
  "license": "ISC",
  "displayName": "SabioApi",
  "dependencies": {
    "@hapi/eslint-config-hapi": "^13.0.2",
    "@hapi/eslint-plugin-hapi": "^4.3.5",
    "@hapi/joi": "^17.1.1",
    "@hapi/rule-capitalize-modules": "^1.2.1",
    "@sendgrid/mail": "^7.2.5",
    "aws-sdk": "^2.739.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.2",
    "commander": "^6.0.0",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "extend": "^3.0.2",
    "formidable": "^1.2.2",
    "fs": "0.0.2",
    "helmet": "^4.0.0",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.27",
    "morgan": "^1.10.0",
    "mssql": "^6.2.1",
    "multer": "^1.4.2",
    "nodemon": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "require-all": "^3.0.0",
    "sabio-debug": "git+https://github.com/sabiocode/debug.git",
    "socket.io": "^2.3.0",
    "uuid": "^8.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run spinup-die"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@ljharb/eslint-config": "^17.1.0",
    "babel-code-frame": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-watch": "git+https://git@github.com/kmagiera/babel-watch.git",
    "del-cli": "^3.0.1",
    "eslint": "^7.6.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-hapi": "^12.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-hapi": "^4.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "install": "^0.13.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5"
  }
}
